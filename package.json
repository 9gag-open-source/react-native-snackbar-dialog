{
  "name": "react-native-snackbar-dialog",
  "version": "1.0.5",
  "description": "A react native snackbar with dialog options",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist --source-maps",
    "start": "node dist/index.js",
    "check": "npm run lint && npm run flow && npm test",
    "lint": "standard --fix",
    "test": "jest",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9gag-open-source/react-native-snackbar-dialog.git"
  },
  "keywords": [
    "snackbar",
    "react",
    "react-native"
  ],
  "author": "9gag Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/9gag-open-source/react-native-snackbar-dialog/issues"
  },
  "babel": {
    "presets": [
      "es2016",
      "react-native"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "expect"
    ]
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "homepage": "https://github.com/9gag-open-source/react-native-snackbar-dialog#readme",
  "dependencies": {
    "react-native-timer": "^1.1.3",
    "babel-cli": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "enzyme": "^2.7.0",
    "flow-bin": "^0.36.0",
    "jest": "^18.1.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.40.0",
    "react-test-renderer": "^15.4.2",
    "standard": "^8.6.0"
  }
}
